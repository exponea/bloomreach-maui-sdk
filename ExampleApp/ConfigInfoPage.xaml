<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ExampleApp.ConfigInfoPage">

    <StackLayout>
        <ScrollView>
            <StackLayout Padding="12">

                <Label Text="Auto session tracking" Margin="0,0,20,0" />
                <Label x:Name="AutomaticSessionTracking" TextColor="#005FDB" />

                <Label Text="Flush mode" Margin="0,0,20,0" />
                <Label x:Name="FlushMode" TextColor="#005FDB" />

                <Label Text="Flush period" Margin="0,0,20,0" />
                <Label x:Name="FlushPeriod" TextColor="#005FDB" />

                <Label Text="Log level" Margin="0,0,20,0" />
                <Label x:Name="LogLevel" TextColor="#005FDB" />

                <Label Text="Default properties" />
                <Label x:Name="DefaultProperties" TextColor="#005FDB" />

            </StackLayout>

        </ScrollView>

    </StackLayout>

</ContentPage>